{
  "parser": "@babel/eslint-parser",
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": [
      "error",
      {
        "useTabs": false,
        "quote-props": "preserve"
      }
    ]
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "requireConfigFile": false
  },
  "extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"]
}
